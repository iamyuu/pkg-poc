{
  "name": "@iamyuu/pkg-poc",
  "type": "module",
  "version": "0.1.3-beta.0",
  "packageManager": "pnpm@8.15.1",
  "description": "_description_",
  "author": "Muhammad Yusuf <https://github.com/iamyuu>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/iamyuu",
  "homepage": "https://github.com/iamyuu/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamyuu/pkg-placeholder.git"
  },
  "bugs": "https://github.com/iamyuu/pkg-placeholder/issues",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "npm -C playground run dev",
    "dev": "pnpm run build --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.51",
    "@vitejs/plugin-react-swc": "3.6.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "happy-dom": "13.3.8",
    "lint-staged": "^15.2.1",
    "pnpm": "^8.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
